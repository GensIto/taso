extend /_includes/_layout
append variables
  - var pageTitle = "";
  - var pageDescription = site.description;
  - var pageKeywords = site.keywords;
  - var pageOgpTitle = "";
  - var pageOgpImage = site.ogpImage
  - var pageLang = "ja";
  - var pageOgpType = "website";
  - var pageUrl = absolutePath;
  - var pagePrefix = "stay";

block content

  //- **********************************************************************
  //- キービジュアル
  //- **********************************************************************
  div.l-kv.l-kv--lg
    div.l-kv__container
      div.p-kv-title
        h1.p-kv-title__item.p-kv-title__item--stay STAY
        p.p-kv-title__p.u-mt2 宿泊施設一覧
    div.l-kv__image
      img(src=`${relativePath}assets/img/activity/kv-marine.jpg` alt="")

  include /_includes/_template/_sp-fixed-btn
  include /_includes/_template/_gnav-screen
  //- **********************************************************************
  //- アクティビティ
  //- **********************************************************************
  section.l-section.l-section--gap_bottom-0.l-section--gap_top-0.u-mt-12.u-mt-sp-4
    img(src=`${relativePath}assets/img/section/purple-w.svg` alt="")
    div.p-decoration
      img.p-decoration__stay(src=`${relativePath}assets/img/stay/text-hotel.png` alt="")
  section.l-section.l-section--gap_top-0.u-pt-sp1
    div.l-container
      div.p-stay-page
        div.p-stay-page__txt.c-fade-up.js-inview
          img.p-stay-page__img-box__img(src=`${relativePath}assets/img/stay/illust-star.png` alt="")
          h2.p-stay-page__txt__title
            span.p-stay-page__txt__title__back HOTEL
            span.p-stay-page__txt__title__front 一棟貸しの場所で素敵なひとときを…
          p.p-stay-page__txt__p どうか腕時計は外して、あなただけの、あなたのための時間と空間を満喫してください。デッキチェアで一日中、部屋に積みっぱなしになっていた本に読みふけるのもよし。木立の隙間から見える美しい海をボーッと眺めているだけでも、充ち足りた気分になることでしょう。お風呂や電気・空調設備もととのった場所でゆったりしたひとときを。

    div.l-container
      p.c-container-title.u-mt8.u-mt-sp4.c-fade-up.js-inview 一棟貸し
      ul.c-camp-list
        li.c-camp-list__item
          h3.c-camp-list__item__title.c-fade-up.js-inview 田曽フォレストキャンプ
          div.c-camp-list__item__wrap.c-fade-up.js-inview
            div.c-camp-list__item__wrap__slider.slider-wrap
              div.swiper-container.c-camp-list-slider.slider
                ul.swiper-wrapper
                  li.swiper-slide
                    img(src=`${relativePath}assets/img/stay/forest-camp/img-camp-1.jpg`  alt='')
                  li.swiper-slide
                    img(src=`${relativePath}assets/img/stay/forest-camp/img-camp-2.jpg`  alt='')
                  li.swiper-slide
                    img(src=`${relativePath}assets/img/stay/forest-camp/img-camp-3.jpg`  alt='')
                  li.swiper-slide
                    img(src=`${relativePath}assets/img/stay/forest-camp/img-camp-4.jpg`  alt='')
                div.c-camp-list-swiper-button-next.swiper-button-next
                div.c-camp-list-swiper-button-prev.swiper-button-prev
                div.c-camp-list-swiper-pagination.swiper-pagination
            div.c-camp-list__item__wrap__content.c-fade-up.js-inview
