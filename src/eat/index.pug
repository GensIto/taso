            img(src=`${relativePath}assets/img/banner/takeout-reservation-pc.jpg` alt='')
  section.l-section.l-section--gap_top-lg.l-section--gap_bottom-0.t-bg-cafe
    div.l-container.l-container--full#bbq
      picture
        img(src=`${relativePath}assets/img/section/brown-white.svg` alt="")
