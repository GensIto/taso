extend /_includes/_layout
append variables
  - var pageTitle = "";
  - var pageDescription = site.description;
  - var pageKeywords = site.keywords;
  - var pageOgpTitle = "";
  - var pageOgpImage = site.ogpImage
  - var pageLang = "ja";
  - var pageOgpType = "website";
  - var pageUrl = absolutePath;
  - var pagePrefix = "eat";

block content

  //- **********************************************************************
  //- キービジュアル
  //- **********************************************************************
  div.l-kv
    div.l-kv__container
      div.p-kv-title
        div.p-kv-title__logo.p-kv-title__logo--md
          img.js-ofi(src=`${relativePath}assets/img/eat/logo.svg` alt="")
        p.p-kv-title__p.u-mt2 Tボーンステーキカフェ
    div.l-kv__movie#eat-video
      div.l-kv__movie__box#youtube-movie-body
        <iframe width="560" height="315" src="https://www.youtube.com/embed/jKjiL0zawxU?autoplay=1&mute=1&playsinline=1&loop=1&playlist=jKjiL0zawxU" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>
      //- img(src=`${relativePath}assets/img/eat/kv.jpg` alt="")
  include /_includes/_template/_sp-fixed-btn
  include /_includes/_template/_gnav-screen
  //- **********************************************************************
  //- enjoy
  //- **********************************************************************
  section.l-section.l-section--gap_bottom-0.l-section--gap_top-0.u-mt-12.u-mt-sp-5
    img(src=`${relativePath}assets/img/section/eat.svg` alt="")
    div.p-decoration
      img.p-decoration__concept-flag.p-decoration__concept-flag--eat(src=`${relativePath}assets/img/news/illust-flag.png` alt="")
    div.p-decoration
      img.p-decoration__enjoy-eat(src=`${relativePath}assets/img/eat/text-eat.png` alt="")
  section.l-section.l-section--gap_top-0.t-bg-light-pink.u-pt-sp8
    div.l-container.l-container--left-full
      div.c-title.c-title--to-r.c-fade-up.js-inview
        h1 BBQ & CAFE
        p バーベキュー & カフェ
      div.p-eat-page.c-fade-up.js-inview
        div.p-eat-page__content
          div.p-eat-page__content__img-box
            div.p-eat-page__content__img-box__label
              img(src=`${relativePath}assets/img/eat/illust-meat.png` alt="")
            img(src=`${relativePath}assets/img/eat/eat-page-main.jpg` alt="")
          div.p-eat-page__content__txt-box
            h2.p-eat-page__content__txt-box__title
              span.p-eat-page__content__txt-box__title__back EAT
              span.p-eat-page__content__txt-box__title__front
                | 手ぶらで楽しめるBBQ♪
                span カフェとしてのご利用も！
            p.p-eat-page__content__txt-box__p.u-mt6 T BONE STEAK CAFE WOOD MEAT STEAK BEERは、三重県 南伊勢町 田曽白浜海岸にあるビーチサイドカフェ。厚切T-BONEステーキを本場アメリカンスタイルで召し上がれ。
            p.p-eat-page__content__txt-box__p また、伊勢の海鮮や豪華なお肉が楽しめる、BBQの食材提供もおこなっております。

  section.l-section.t-bg-light-pink
    div.l-container
      div.p-eat-page__bottom
        ul.p-eat-page__bottom__cards
          li.p-eat-page__bottom__cards__item.c-fade-up.js-inview
