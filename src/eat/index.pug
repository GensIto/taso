            div.p-eat-page-section__content__img-box__label
              img(src=`${relativePath}assets/img/eat/illust-drink.png` alt="")
            picture
              img.p-eat-page-section__content__img-box__img(src=`${relativePath}assets/img/enjoy/meat.jpg` alt="")
  section.l-section.t-bg-cafe
    div.l-container
      div.p-eat-page-bottom
        div.p-eat-page-bottom__top#lunch.c-fade-up.js-inview
