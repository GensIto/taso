              li.p-eat-page__bottom__cards__item__link-list__item
                a.p-eat-page__bottom__cards__item__inner(href=`${relativePath}eat/cafe`)
                  p.p-eat-page__bottom__cards__item__link-list__item__txt カフェ
              li.p-eat-page__bottom__cards__item__link-list__item
                a.p-eat-page__bottom__cards__item__inner(href=`${relativePath}eat/#morning`)
                  p.p-eat-page__bottom__cards__item__link-list__item__txt 朝食
          li.p-eat-page__bottom__cards__item.c-fade-up.js-inview
            a.p-eat-page__bottom__cards__item__inner.p-eat-page__bottom__cards__item__inner--col(href="#")
